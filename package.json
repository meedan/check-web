    "chai": "^3.5.0",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "jsdom": "^8.5.0",
    "mocha": "^7.1.1",
    "mocha-webpack": "^2.0.0-beta.0",
