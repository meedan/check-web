{
  "name": "Check",
  "version": "0.0.1",
  "description": "Verify breaking news online",
  "private": true,
  "repository": {
    "type": "git",
    "url": "git://github.com/meedan/checkdesk-client.git"
  },
  "scripts": {
    "logo": "cd src/assets/images/logo && ./generate.sh && cd ..",
    "style": "node-sass src/app/styles/stylesheet.scss --output-style=compressed | postcss --use autoprefixer -o build/web/css/stylesheet.css && cp -r src/app/styles/images build/web/",
    "style:rebuild": "npm rebuild node-sass && mkdir -p build/web/css 2>/dev/null && npm run style",
    "build": "rm -rf build/web && npm run style && npm run build:gulp",
    "build:gulp": "npm run transifex:download && BABEL_ENV=production gulp build:web && npm run transifex:upload",
    "transifex:download": "gulp transifex:download && gulp transifex:translations",
    "transifex:upload": "gulp transifex:prepare && gulp transifex:upload",
    "watch:styles": "watch 'npm run style' ./src/app/styles",
    "publish": "node scripts/server.js",
    "test": "npm run test:unit; npm run test:integration",
    "test:unit": "cp test/unit/config.js node_modules/config.webpack-node.js && NODE_PATH=$NODE_PATH:. BABEL_ENV=production mocha-webpack --webpack-config webpack/test.babel.js --require test/unit/setup.js \"test/unit/*.spec.js\"",
    "test:integration": "cd test && rspec spec/integration_spec.rb && cd ..",
    "test:proxy": "cd test && ruby spec/sauce/sauce_connect.rb && cd ..",
    "test:browsers": "cd test && rspec spec/browser_spec.rb && cd ..",
    "test:browsers:parallel": "cd test && PARALLEL_SPLIT_TEST_PROCESSES=2 parallel_split_test spec/browser_spec.rb && cd ..",
    "test:ff": "cd test && rspec --fail-fast && cd ..",
    "lint:styles": "sass-lint --config .sass-lint.yml './**/*.scss' --verbose --ignore './test/**/*.scss, ./node_modules/**/*.scss'",
    "lint:src": "eslint src --fix"
  },
  "keywords": [
    "react",
    "reactjs",
    "redux",
    "checkmedia"
  ],
  "authors": [
    "Meedan"
  ],
  "license": "MIT",
  "devDependencies": {
    "babel-plugin-transform-runtime": "^6.15.0",
    "chai": "^3.5.0",
    "chromedriver": "2.25.3",
    "enzyme": "^2.7.1",
    "eslint": "^3.12.2",
    "eslint-config-airbnb": "^13.0.0",
    "eslint-plugin-import": "^2.2.0",
